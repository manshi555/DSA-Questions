public class Main {
    public static void main(String[] args) {
      String s = "abcdeabcfcz";
      int hash[] = new int[26];//0-> 25 index
      //pre-computing
      for(int i=0; i< s.length(); i++)
      {
        hash[s.charAt(i)-'a']++;// a->0,b->1,c->2.........z->25
      }
      for(int i=0; i< hash.length; i++)
      {
        System.out.print(hash[i]+" ");
      }
      
  }
